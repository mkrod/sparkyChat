#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules/peer/dist/bin/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules/peer/dist/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules/peer/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules/peer/dist/bin/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules/peer/dist/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules/peer/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/peer@1.0.2/node_modules:/home/mkrod/Projects/chatApp/peerjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/peerjs.js" "$@"
else
  exec node  "$basedir/../../dist/bin/peerjs.js" "$@"
fi
